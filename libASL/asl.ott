slice :: 'Slice_' ::=
    {{ pp es = match es with
        | Slice_Single expression -> string "{\"node_type\":\"Slice_\",\"node_subtype\":\"Single\",\"node_data\":{\"expr\":"
            ^^ pp_expr expression
            ^^ string "}"
            ^^ string "}"
