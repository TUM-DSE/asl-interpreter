unop :: 'Unop_' ::=
    {{ pp es = match es with
        | Unop_Negate -> string "{\"node_type\":\"Unop_\",\"node_subtype\":\"Negate\"}"
        | Unop_BoolNot -> string "{\"node_type\":\"Unop_\",\"node_subtype\":\"BoolNot\"}"
        | Unop_BitsNot -> string "{\"node_type\":\"Unop_\",\"node_subtype\":\"BitsNot\"}" }}
    | -        :: :: Negate
    | !        :: :: BoolNot
