        | Binop_Multiply -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Multiply\"}"
        | Binop_Divide -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Divide\"}"
        | Binop_Power -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Power\"}"
        | Binop_Quot -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Quot\"}"
        | Binop_Rem -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Rem\"}"
        | Binop_Div -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Div\"}"
        | Binop_Mod -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Mod\"}"
        | Binop_ShiftL -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"ShiftL\"}"
        | Binop_ShiftR -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"ShiftR\"}"
        | Binop_BoolAnd -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"BoolAnd\"}"
        | Binop_BoolOr -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"BoolOr\"}"
        | Binop_BoolIff -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"BoolIff\"}"
        | Binop_BoolImplies -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"BoolImplies\"}"
        | Binop_BitOr -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"BitOr\"}"
        | Binop_BitEor -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"BitEor\"}"
        | Binop_BitAnd -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"BitAnd\"}"
        | Binop_Append -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Append\"}"
        | Binop_Concat -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"Concat\"}"
        | Binop_DUMMY -> string "{\"node_type\":\"Binop_\",\"node_subtype\":\"DUMMY\"}" }}
    | ==      :: :: Eq
    | !=      :: :: NtEq
    | >       :: :: Gt
    | >=      :: :: GtEq
    | <       :: :: Lt
    | <=      :: :: LtEq
    | +       :: :: Plus
